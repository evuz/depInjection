{
  "name": "depsin",
  "version": "1.2.4",
  "license": "ISC",
  "description": "A lightweight library for dependency injection in JavaScript",
  "author": "Jesus Gomez Bellido",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evuz/depInjection.git"
  },
  "main": "dist/depsin.cjs.js",
  "module": "dist/depsin.esm.js",
  "browser": "dist/depsin.umd.js",
  "types": "dist/depsin.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "jest --watch",
    "test": "jest --silent",
    "prepare": "npm run build",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "rollup": "^1.10.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
