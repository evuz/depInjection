{
  "name": "depsin",
  "version": "2.0.0",
  "license": "ISC",
  "description": "A lightweight library for dependency injection in JavaScript",
  "author": "Jesus Gomez Bellido",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evuz/depInjection.git"
  },
  "main": "dist/depsin.cjs.js",
  "module": "dist/depsin.esm.js",
  "browser": "dist/depsin.umd.js",
  "types": "dist/depsin.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "jest --watch",
    "test": "jest --silent",
    "prepare": "npm run build",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.14",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
